{
  "name": "mstools",
  "version": "0.1.0",
  "description": "Tools using wasm & javascript",
  "main": "index.js",
  "bin": {
    "create-wasm-app": ".bin/create-wasm-app.js"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js  --mode production",
    "start": "webpack-dev-server --mode production development",
    "deploy": "npm run build && touch dist/.nojekyll && gh-pages --dotfiles=true --dist dist/"
  },
  "keywords": [
    "webassembly",
    "wasm",
    "rust",
    "webpack"
  ],
  "dependencies": {
    "wasm-mstools": "file:../pkg"
  },
  "devDependencies": {
    "webpack": "^4.29.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "copy-webpack-plugin": "^5.0.0",
    "gh-pages": "^3.2.3"
  }
}
