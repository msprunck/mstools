{
  "name": "mstools",
  "version": "0.1.0",
  "description": "Tools using wasm & javascript",
  "scripts": {
    "build": "npm run clean:build && webpack --config webpack.config.js --mode production",
    "clean": "npm run clean:build",
    "clean:build": "rimraf dist",
    "deploy": "npm run build && touch dist/.nojekyll && gh-pages --dotfiles=true --dist dist/",
    "start": "webpack serve --mode development"
  },
  "keywords": [
    "webassembly",
    "wasm",
    "rust",
    "webpack"
  ],
  "dependencies": {
    "wasm-mstools": "file:../pkg"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "gh-pages": "^3.2.3",
    "mini-css-extract-plugin": "^2.5.3",
    "postcss": "^8.4.6",
    "postcss-loader": "^6.2.1",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.23",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
